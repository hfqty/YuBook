<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application

        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:exported="true"
            android:name="me.zackyu.yubook.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="me.zackyu.yubook.NewRecordActivity"></activity>
        <activity android:name="me.zackyu.yubook.RecordsActivity"></activity>
        <activity android:name="me.zackyu.yubook.NewPayActivity"></activity>
        <activity android:name="me.zackyu.yubook.NewIncomeActivity"></activity>
        <activity android:name="me.zackyu.yubook.AboutMeActivity"></activity>
    </application>

</manifest>